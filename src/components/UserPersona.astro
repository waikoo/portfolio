---
const { className } = Astro.props;
---

<picture>
  <source type="image/avif" srcset="/images/user_persona.avif" />
  <source type="image/webp" srcset="/images/user_persona.webp" />
  <source type="image/jpg" srcset="/images/user_persona.jpg" />
  <img
    src="/images/user_persona.png"
    alt="user persona"
    class={`user-persona ${className}`}
  />
</picture>
