---
const { imgSelector } = Astro.props;
---

<picture>
  <source srcset="/images/quizzical_default_user_flow.avif" type="image/avif" />
  <source srcset="/images/quizzical_default_user_flow.webp" type="image/webp" />
  <img
    src="/images/quizzical_default_user_flow.png"
    alt="Quizzical Default User Flow"
    class={imgSelector}
  />
</picture>
